<?xml version="1.0" encoding="utf-8"?>
<configuration>
<system.serviceModel>
  <services>
    <service name="Library.Service.Server">
      <endpoint address="" binding="webHttpBinding" contract="Library.Service.IServer" behaviorConfiguration="webBehavior">
      </endpoint>
    </service>
  </services>
  <behaviors>
    <endpointBehaviors>
      <behavior name="webBehavior">
        <webHttp />
      </behavior>
    </endpointBehaviors>
    <serviceBehaviors>
      <behavior name="">
        <serviceMetadata httpGetEnabled="true" httpsGetEnabled="true" />
        <serviceDebug includeExceptionDetailInFaults="false" />
      </behavior>
    </serviceBehaviors>
  </behaviors>
  <serviceHostingEnvironment aspNetCompatibilityEnabled="true"
    multipleSiteBindingsEnabled="true" />
  </system.serviceModel>
</configuration>
